{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/pages/**/*.html",
            "use": "@vercel/static"
        },
        {
            "src": "frontend/css/**/*.css",
            "use": "@vercel/static"
        },
        {
            "src": "frontend/js/**/*.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/admin",
            "dest": "/frontend/pages/admin.html"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1.js"
        },
        {
            "src": "/css/(.*)",
            "dest": "/frontend/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/frontend/js/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/pages/index.html"
        }
    ],
