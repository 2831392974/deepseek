{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/**/*",
            "use": "@vercel/static",
            "config": {
                "public": "frontend"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/$1.js"
        },
        {
            "src": "/css/(.*)",
            "dest": "css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "js/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "js/$1"
        },
        {
            "src": "/admin",
            "dest": "pages/admin.html"
        },
        {
            "src": "/",
            "dest": "pages/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
